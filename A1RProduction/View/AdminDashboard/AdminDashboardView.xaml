    <UserControl.Triggers>
        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="HomeTextBlock">
            <BeginStoryboard x:Name="homeMouseEnter" Storyboard="{StaticResource homeMouseEnter}" />
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="HomeTextBlock">
            <BeginStoryboard x:Name="homeMouseLeave" Storyboard="{StaticResource homeMouseLeave}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="MaintenanceTextBlock">
            <BeginStoryboard x:Name="MaintenanceMouseEnter" Storyboard="{StaticResource MaintenanceMouseEnter}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="MaintenanceTextBlock">
            <BeginStoryboard x:Name="MaintenanceMouseLeve" Storyboard="{StaticResource MaintenanceMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="OrdersTextBlock">
            <BeginStoryboard x:Name="OrdersMouseEnter" Storyboard="{StaticResource OrdersMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="OrdersTextBlock">
            <BeginStoryboard x:Name="OrdersMouseLeve" Storyboard="{StaticResource OrdersMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="ProductionMaintenanceTextBlock">
            <BeginStoryboard x:Name="ProductionMaintenanceMouseEnter" Storyboard="{StaticResource ProductionMaintenanceMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="ProductionMaintenanceTextBlock">
            <BeginStoryboard x:Name="ProductionMaintenanceMouseLeve" Storyboard="{StaticResource ProductionMaintenanceMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="StockTextBlock">
            <BeginStoryboard x:Name="StockMouseEnter" Storyboard="{StaticResource StockMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="StockTextBlock">
            <BeginStoryboard x:Name="StockMouseLeve" Storyboard="{StaticResource StockMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="CustomerTextBlock">
            <BeginStoryboard x:Name="CustomerMouseEnter" Storyboard="{StaticResource CustomerMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="CustomerTextBlock">
            <BeginStoryboard x:Name="CustomerMouseLeve" Storyboard="{StaticResource CustomerMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="OffSpecReportTextBlock">
            <BeginStoryboard x:Name="OffSpecReportMouseEnter" Storyboard="{StaticResource OffSpecReportMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="OffSpecReportTextBlock">
            <BeginStoryboard x:Name="OffSpecReportMouseLeve" Storyboard="{StaticResource OffSpecReportMouseLeve}"/>
        </EventTrigger>

        <EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="IBCTextBlock">
            <BeginStoryboard x:Name="IBCMouseEnter" Storyboard="{StaticResource IBCMouseEnter}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="IBCTextBlock">
            <BeginStoryboard x:Name="IBCMouseLeve" Storyboard="{StaticResource IBCMouseLeve}"/>
        </EventTrigger>

    </UserControl.Triggers>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="{Binding Version}" Padding="7,3,0,0" FontSize="10" FontWeight="Bold" Margin="0,0,0,7" Foreground="White" Height="22" VerticalAlignment="Bottom" Grid.Column="0" Background="#FF6A6868" Grid.ColumnSpan="6" />
        <TextBlock Background="#FF938F8F" Foreground="White" Padding="0,3,0,0" Margin="0,37,0,0" TextWrapping="Wrap" Text="Admin Dashboard" TextAlignment="Center" FontWeight="Bold" FontSize="20" FontFamily="Arial" Height="32" VerticalAlignment="Top" Grid.ColumnSpan="6"/>

        <Grid Margin="8,7,0,0" Height="26" VerticalAlignment="Top" Grid.ColumnSpan="6">
            <TextBlock x:Name="HomeTextBlock" Style="{StaticResource HoverEffect}" HorizontalAlignment="Left" Width="80" FontSize="12" Text="DASHBOARD" TextWrapping="Wrap" TextAlignment="Center" RenderTransformOrigin="0.5,0.5" Height="18" VerticalAlignment="Top" Margin="0,2,0,0">
                <TextBlock.InputBindings>
                    <MouseBinding Command="{Binding NavHomeCommand}" MouseAction="LeftClick" />
                </TextBlock.InputBindings>
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>

            <Label HorizontalAlignment="Left" Height="22" VerticalAlignment="Top" Width="17" Margin="91,0,0,0">
                <Label.Background>
                    <ImageBrush ImageSource="/A1QSystem;component/Images/NavArrow.png"/>
                </Label.Background>
            </Label>
            <TextBlock x:Name="NewQuoteTextBlock" Padding="1,1.5,0,1"  HorizontalAlignment="Left" Width="149" FontSize="13" Text="ADMIN DASHBOARD" TextWrapping="Wrap" TextAlignment="Center" RenderTransformOrigin="0.5,0.5" Height="22" VerticalAlignment="Top" Background="#FFC33333" Foreground="White" FontWeight="Bold" Margin="110,0,0,0" />
        </Grid>
        <TextBlock TextAlignment="Center" Margin="0,280,30,0" VerticalAlignment="Top" Text="Maintenance" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" Grid.Column="1" />
        <TextBlock x:Name="MaintenanceTextBlock" TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,280,30,0" VerticalAlignment="Top" Text="Maintenance" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0" Grid.Column="1">
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding MaintenanceCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,107,31,0" VerticalAlignment="Top" Text="Orders" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" />
        <TextBlock x:Name="OrdersTextBlock" Visibility="{Binding OrdersVisiblity}" TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,107,31,0" VerticalAlignment="Top" Text="Orders" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0">
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding OrdersCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,280,31,0" VerticalAlignment="Top" Text="Production" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" />
        <TextBlock x:Name="ProductionMaintenanceTextBlock" TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,280,31,0" VerticalAlignment="Top" Text="Production" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0" 
                   Visibility="{Binding ProductionMaintenanceVisibility}" >
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding ProdMaintenanceCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,107,30,0" VerticalAlignment="Top" Text="Stock" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" Grid.Column="1" />
        <TextBlock x:Name="StockTextBlock"  TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,107,30,0" VerticalAlignment="Top" Text="Stock" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0" Grid.Column="1" Visibility="{Binding StockVisiblity}">
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding StockCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,459,31,0" VerticalAlignment="Top" Text="Customer" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" />
        <TextBlock x:Name="CustomerTextBlock" TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,459,31,0" VerticalAlignment="Top" Text="Customer" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0" 
        	Visibility="{Binding ProductionMaintenanceVisibility}" >
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding CustomerCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,107,30
                   ,0" VerticalAlignment="Top" Text="Slitting Off-Spec Report" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,44,0,0" Grid.Column="2" />
        <TextBlock x:Name="OffSpecReportTextBlock"  TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,107,30,0" VerticalAlignment="Top" Text="Slitting Off-Spec Report" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,44,0,0" Grid.Column="2" Visibility="{Binding StockVisiblity}">
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding OffSpecCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock TextAlignment="Center" Margin="0,280,30,0" VerticalAlignment="Top" Text="IBC Change Over" TextWrapping="Wrap" FontSize="16"  RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right"  Foreground="#FFD6CECE" Height="128" Width="130" Background="WhiteSmoke" Padding="0,50,0,0" Grid.Column="2" />
        <TextBlock x:Name="IBCTextBlock" TextAlignment="Center" Style="{StaticResource HoverEffect}" Margin="0,280,30,0" VerticalAlignment="Top" Text="IBC Change Over" TextWrapping="Wrap" FontSize="16" Height="128" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Right" Width="130" Padding="0,50,0,0" Grid.Column="2">
            <TextBlock.InputBindings>
                <MouseBinding Command="{Binding IBCChangeCommand}" MouseAction="LeftClick"  />
            </TextBlock.InputBindings>
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <!--<Border x:Name="childWindow" Visibility="{Binding WindowVisibility}" Background="#88000000" Margin="0,0,0,7" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="6" >
            <ContentControl Content="{Binding XmlContent}"  HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"/>
        </Border>-->

    </Grid>
</UserControl>
