            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="WeeklyScheduleTextBlock" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
